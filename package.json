{
  "name": "my-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "generate-migrations": "drizzle-kit generate:sqlite --config=./drizzle.config.ts",
    "migrate": "bunx tsx ./scripts/migrate.ts",
    "db:seed-states": "bunx tsx ./scripts/seed-states.ts",
    "db:seed-events": "bunx tsx ./scripts/seed-events.ts",
    "db:clear": "bunx tsx ./scripts/clear-db.ts"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.564.0",
    "@aws-sdk/s3-request-presigner": "^3.564.0",
    "@libsql/client": "^0.6.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/typography": "^0.5.13",
    "@types/eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.20.17",
    "drizzle-orm": "^0.30.9",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "^8.4.38",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "slugify": "^1.6.6",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "sveltekit-flash-message": "^2.4.4",
    "tailwindcss": "^3.4.3",
    "ts-debounce": "^4.0.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3"
  },
  "type": "module",
  "dependencies": {
